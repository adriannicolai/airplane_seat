<form action="/airplane_seats" id="add_seat_form" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Type the array here</p>
  <input type="text" name="airplane_seat[seat_array]">
  <p>Type the number of passengers here</p>
  <input type="text" name="airplane_seat[passenger_count]">
  <input type="submit" value="Send">
</form>